{
    "builds": [
        {
            "src": "app.py",
            "use": "@streamlit/vercel-plugin"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install && pip install -r requirements.txt && playwright install chromium",
    "framework": "nextjs",
    "outputDirectory": ".next"
} 